{
	"authors": [
		"ew"
	],
	"copyright": "Copyright Â© 2025, ew",
	"description": "A static site generator written in D",
	"license": "AGPL-3.0-or-later",
	"name": "ssgd",
	"dependencies": {
		"commonmark-d": "~>1.0.10",
		"commandr": "~>1.1.0",
		"diet-ng": "~>1.8.4",
		"vibe-d": "~>0.10.2"
	},
	"buildTypes": {
		"release": {
			"buildOptions": ["releaseMode", "optimize", "inline"],
			"dflags": ["-link-defaultlib-shared=false"]
		}
	},
	"configurations": [
		{
			"name": "executable",
			"targetType": "executable"
		},
		{
			"name": "unittest",
			"targetType": "executable",
			"sourcePaths": ["source", "tests"],
			"importPaths": ["source", "tests"]
		}
	]
}
